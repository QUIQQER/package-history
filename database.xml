<?xml version="1.0" encoding="UTF-8"?>
<database>

    <projects>
        <table name="archiv" no-auto-update="1" no-site-reference="1">
            <field type="BIGINT(20) NOT NULL">id</field>
            <field type="DATETIME NOT NULL">created</field>
            <field type="LONGTEXT NULL">data</field>
            <field type="INT(11)">uid</field>

            <primary>id,created</primary>
        </table>

        <table name="history_bricks" no-auto-update="1" no-site-reference="1">
            <field type="INT(3) NOT NULL">id</field>
            <field type="DATETIME NOT NULL">created</field>
            <field type="LONGTEXT NULL">data</field>
            <field type="INT(11)">uid</field>

            <primary>id,created</primary>
        </table>
    </projects>

</database>
